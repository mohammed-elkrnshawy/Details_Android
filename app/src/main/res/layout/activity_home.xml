<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:tools="http://schemas.android.com/tools"    xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    xmlns:app="http://schemas.android.com/apk/res-auto"    tools:context=".PackageHome.HomeActivity">    <androidx.appcompat.widget.Toolbar        android:layout_marginStart="-20dp"        android:id="@+id/toolbar"        android:background="@color/ColorWhite"        android:layout_width="match_parent"        android:layout_height="100dp"        />    <FrameLayout        android:layout_below="@id/toolbar"        android:background="@color/ColorGray"        android:layout_above="@id/navigation"        android:id="@+id/conainerHome"        android:layout_width="match_parent"        android:layout_height="match_parent"></FrameLayout>    <com.google.android.material.bottomnavigation.BottomNavigationView        android:id="@+id/navigation"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentBottom="true"        android:background="?android:attr/windowBackground"        app:itemBackground="@color/ColorWhite"        android:foreground="?attr/selectableItemBackground"        app:itemIconTint="@drawable/shape_bottom_nav_item_style"        app:itemTextColor="@drawable/shape_bottom_nav_item_style"        app:menu="@menu/bottom_menu" /></RelativeLayout>